{
  "name": "@aquilacms/storefront-data-hooks",
  "version": "1.0.0",
  "description": "Next.js Commerce Framework",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf *.js *.d.ts api cart commerce products scripts wishlist",
    "clean:install": "rimraf node_modules yarn.lock && yarn install",
    "build": "npm run clean && tsc && cp src/schema.d.ts schema.d.ts",
    "lint": "eslint . --quiet --fix --ext .js,.ts,.tsx"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "account",
    "api",
    "auth",
    "cart",
    "commerce",
    "products",
    "wishlist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aquilacms/storefront-data-hooks.git"
  },
  "keywords": [
    "nextjs",
    "commerce"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aquilacms/storefront-data-hooks/issues"
  },
  "homepage": "https://github.com/aquilacms/storefront-data-hooks#readme",
  "dependencies": {
    "@vercel/fetch": "6.1.0",
    "cookie": "0.4.1",
    "js-cookie": "2.2.1",
    "lodash.debounce": "4.0.8",
    "swr": "0.3.11"
  },
  "devDependencies": {
    "@manifoldco/swagger-to-ts": "^2.1.0",
    "@types/cookie": "^0.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "husky": "^4.3.7",
    "lint-staged": "^10.5.3",
    "next": "^10.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
